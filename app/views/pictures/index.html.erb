

<p id="notice"><%= notice %></p>

<h1>Welcome to Photogur!</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pictures.each do |picture| %>
      <tr>

       
        <td><%= picture.title %></td>
        <td><a href = "<%= picture_url(picture) %>"><%= image_tag picture.url %></a></td>
        <%# <td><%= link_to 'Show', picture %></td> 
        <%# <td><%= link_to 'Edit', edit_picture_path(picture) %></td>
        <%# <td><%= link_to 'Destroy', picture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Link to a new photo!', new_picture_path %>
